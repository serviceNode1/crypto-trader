{
  "name": "crypto-ai-trading",
  "version": "1.0.0",
  "description": "AI-powered cryptocurrency analysis and paper trading system",
  "main": "dist/app.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/app.ts",
    "dev:fresh": "npx kill-port 3000 && npm run dev",
    "restart": "npx kill-port 3000 && npm run dev",
    "build": "tsc",
    "start": "node dist/app.js",
    "migrate": "ts-node src/migrations/run.ts",
    "migrate:cleanup": "ts-node src/scripts/cleanupMigrations.ts",
    "verify-schema": "ts-node src/scripts/verifySchema.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "analyze": "ts-node src/scripts/analyze.ts",
    "discover": "ts-node src/scripts/discover.ts",
    "report": "ts-node src/scripts/report.ts",
    "backtest": "ts-node src/scripts/backtest.ts",
    "train": "ts-node src/scripts/train.ts",
    "test:apis": "ts-node src/scripts/testApis.ts",
    "test:binance": "ts-node src/scripts/testBinance.ts",
    "test:coinbase": "ts-node src/scripts/testCoinbaseAuth.ts",
    "test:integrations": "ts-node src/scripts/testIntegrations.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "crypto",
    "trading",
    "ai",
    "paper-trading",
    "bitcoin"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.65.0",
    "axios": "^1.6.2",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "lodash": "^4.17.21",
    "natural": "^6.10.0",
    "node-fetch": "^2.7.0",
    "openai": "^4.20.1",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "rss-parser": "^3.13.0",
    "sentiment": "^5.0.2",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/natural": "^5.1.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.15.5",
    "@types/sentiment": "^5.0.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
